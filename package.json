{
  "name": "dev-go",
  "displayName": "DevGo",
  "version": "0.0.3",
  "description": "A super unbeatable browser extension plugin for developers.",
  "author": "荣顶",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "build:zip": "plasmo build -- --zip"
  },
  "dependencies": {
    "antd": "^5.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.5.4",
    "@types/chrome": "0.0.198",
    "@types/node": "18.11.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.13",
    "plasmo": "0.59.1",
    "postcss": "^8.4.19",
    "prettier": "2.8.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.8.4"
  },
  "homepage": "https://github.com/wangrongding",
  "manifest": {
    "homepage_url": "https://github.com/wangrongding",
    "permissions": [
      "contextMenus"
    ],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Alt+Q",
          "windows": "Alt+Q",
          "mac": "Alt+Q",
          "chromeos": "Alt+Q",
          "linux": "Alt+Q"
        }
      },
      "inline-translate": {
        "suggested_key": {
          "default": "Ctrl+Shift+E",
          "windows": "Ctrl+Shift+E",
          "chromeos": "Ctrl+Shift+E",
          "linux": "Ctrl+Shift+E",
          "mac": "Command+Shift+E"
        },
        "description": "inline-translate"
      }
    }
  }
}
